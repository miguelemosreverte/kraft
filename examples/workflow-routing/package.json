{
  "name": "kraft-workflow-routing",
  "version": "1.0.0",
  "description": "Kraft Workflow Routing Demo",
  "type": "module",
  "scripts": {
    "start": "npx tsx index.ts",
    "start:remote": "KRAFT_NODES=http://192.168.0.130:8800,http://192.168.0.130:8801 npx tsx index.ts",
    "start:local": "KRAFT_NODES=http://localhost:8800,http://localhost:8801 npx tsx index.ts"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
